cat > .gitignore <<'EOF'
# Virtual envs
venv/
.venv/
env/
ENV/

# Python cache/build artifacts
__pycache__/
*.py[cod]
*.so
*.dylib
*.pyd
*.egg-info/
dist/
build/

# OS/IDE
.DS_Store
.vscode/
.idea/
EOF

git add .gitignore
git commit -m "Ignore venv and build artifacts"

git push -u origin main --force